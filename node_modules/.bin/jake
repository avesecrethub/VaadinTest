#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/jake@10.8.2/node_modules/jake/bin/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/jake@10.8.2/node_modules/jake/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/jake@10.8.2/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules:/mnt/c/Users/vaneyar/Projects/node_modules:/mnt/c/Users/vaneyar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/jake/bin/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/jake/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/jake@10.8.2/node_modules/jake/bin/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/jake@10.8.2/node_modules/jake/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/jake@10.8.2/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules:/mnt/c/Users/vaneyar/Projects/node_modules:/mnt/c/Users/vaneyar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/jake/bin/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/jake/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../jake/bin/cli.js" "$@"
fi
