#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/fixtures/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/import-local@3.0.3/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules:/mnt/c/Users/vaneyar/Projects/node_modules:/mnt/c/Users/vaneyar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/webpack-cli@4.9.0_714d72df2bebb3ac089d7453aabd5d57/node_modules/webpack-cli/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/webpack-cli@4.9.0_714d72df2bebb3ac089d7453aabd5d57/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/fixtures/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/import-local@3.0.3/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules:/mnt/c/Users/vaneyar/Projects/node_modules:/mnt/c/Users/vaneyar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/webpack-cli@4.9.0_714d72df2bebb3ac089d7453aabd5d57/node_modules/webpack-cli/node_modules:/mnt/c/Users/vaneyar/Projects/VaadinTest/node_modules/.pnpm/webpack-cli@4.9.0_714d72df2bebb3ac089d7453aabd5d57/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@3.0.3/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@3.0.3/node_modules/import-local/fixtures/cli.js" "$@"
fi
